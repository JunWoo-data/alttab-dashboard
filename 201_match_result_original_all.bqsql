create or replace table `alt-tab-348721.alttab_all.match_result_original_all`
partition by match_date
as

select * from (
    SELECT * FROM `alt-tab-348721.alttab_21f.match_result_original`
    union all 
    SELECT * FROM `alt-tab-348721.alttab_22w.match_result_original`
    union all
    SELECT * FROM `alt-tab-348721.alttab_22f.match_result_original`
)
where season is not null