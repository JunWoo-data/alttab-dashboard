create table `alttab-348505.alttab_22w.player_rank`
partition by match_date 
as

select
  player_name, 
  match_date,
  case when match_date = "2021-01-05" then 200 else null end as rank_point,
  case when match_date  = "2021-01-05" then 1 else null end as rank
from (
  SELECT * FROM `alttab-348505.alttab_22w.player_name_list` 
  cross join (select * from `alttab-348505.alttab_22w.season_date_list`)
)
order by match_date